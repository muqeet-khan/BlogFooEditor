<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BlogFooEditor.ShowResult"
             Title="Preview Markdown">
  <ContentPage.ToolbarItems>
    <ToolbarItem Name="Upload" x:Name="Upload"></ToolbarItem>
  </ContentPage.ToolbarItems>
  <ContentPage.Content>
    <ScrollView>
    <StackLayout>
      <WebView x:Name="browser" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" HeightRequest="600">
      </WebView>
      <StackLayout x:Name="ImageStack" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
      </StackLayout>
    </StackLayout>
    </ScrollView>
  </ContentPage.Content>
</ContentPage>